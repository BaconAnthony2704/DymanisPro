<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gestionclientes">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_peque"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_peque"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".ConsultarCliente"></activity>
        <activity android:name=".BorrarActivity" />
        <activity android:name=".EditClientes" />
        <activity android:name=".gestion.GestionPartnerActivity" />
        <activity android:name=".RegistrarsePartnerActivity" />
        <activity
            android:name=".WelcomeActivity"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".gestion.GestionAdministradorActivity" />
        <activity android:name=".gestion.GestionListaClienteActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />

    <uses-permission android:name="android.permission.READ_PROFILE" />

</manifest>